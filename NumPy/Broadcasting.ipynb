   "source": [
    "#Broadcasting\n",
    "import numpy as np\n",
    "var = np.array([1,2,3,4])\n",
    "print(var.shape)\n",
    "print(var)\n",
    "\n",
    "\n",
    "var1 = np.array([[1],[2],[3]])\n",
    "print(var1.shape)\n",
    "print()\n",
    "print(var1)\n",
    "print()\n",
    "print(var + var1)"
   ]
  },
